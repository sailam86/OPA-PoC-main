{
  "installCommand": "pnpm install",
  "buildCommand": "pnpm node scripts/build.js && mkdir -p public && touch public/index.html",
  "outputDirectory": "public",
  "rewrites": [
    {
      "source": "/(host|farmerLead|farmer|dashboard)",
      "destination": "api/index.js"
    }
  ],
  "github": {
    "enabled": false
  }
}
